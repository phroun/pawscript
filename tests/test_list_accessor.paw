# Test list accessor syntax

echo "=== Testing dot accessor syntax for named arguments ==="
x: {list a: 10, b: 20, c: 30}
echo "x.a =", ~x.a
echo "x.b =", ~x.b
echo "x.c =", ~x.c

echo ""
echo "=== Testing index accessor syntax for positional items ==="
y: {list first, second, third}
echo "y 0 =", ~y 0
echo "y 1 =", ~y 1
echo "y 2 =", ~y 2

echo ""
echo "=== Testing chained accessors ==="
z: {list inner: {list val: 42}}
echo "z.inner.val =", ~z.inner.val

echo ""
echo "=== Testing multidimensional arrays ==="
# A 2x3 matrix: [[1,2,3], [4,5,6]]
matrix: {list {list 1, 2, 3}, {list 4, 5, 6}}
echo "matrix 0 0 =", ~matrix 0 0
echo "matrix 0 2 =", ~matrix 0 2
echo "matrix 1 1 =", ~matrix 1 1

echo ""
echo "=== Testing positional arrays within named elements ==="
data: {list items: {list apple, banana, cherry}, count: 3}
echo "data.items 0 =", ~data.items 0
echo "data.items 1 =", ~data.items 1
echo "data.items 2 =", ~data.items 2
echo "data.count =", ~data.count

echo ""
echo "=== Testing index then dot accessor ==="
students: {list {list firstName: "Alice", lastName: "Smith"}, {list firstName: "Bob", lastName: "Jones"}}
echo "students 0.firstName =", ~students 0.firstName
echo "students 1.lastName =", ~students 1.lastName

echo ""
echo "=== Testing undefined access ==="
echo "x.nonexistent =", ~x.nonexistent
echo "y 99 =", ~y 99

echo ""
echo "=== Test complete ==="
