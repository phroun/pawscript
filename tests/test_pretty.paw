#nested: {list 1, 2, {list a: 1, b: 2}, name: "test"}
echo "{json ~#nested, mode: explicit, pretty: true}"
echo ""

# Test 2: Compare with non-pretty
echo "Test 2: Same structure - compact JSON"
echo "{json ~#nested, mode: explicit}"
echo ""

# Test 3: String command with pretty
echo "Test 3: String command - pretty"
echo "{string ~#nested, pretty: true}"
echo ""

# Test 4: String command without pretty
echo "Test 4: String command - compact"
echo "{string ~#nested}"
echo ""

