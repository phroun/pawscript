echo ""

# Test 6: Deep nesting - compact JSON
echo "Test 6: Deep nesting - compact JSON"
echo "{json ~#deep, mode: explicit}"
echo ""

# Test 7: Named-only structure - pretty JSON
echo "Test 7: Named-only - pretty JSON"
#config: {list server: "localhost", port: 8080, debug: true, options: {list timeout: 30, retries: 3}}
