# Complex chains
echo "Step 1"
& echo "Step 2 (conditional)"
& echo "Step 3 (conditional)"  
| echo "Fallback (if any step failed)"
; echo "Final step (always runs)"

echo "Step 1" &
echo "Step 2 (conditional)" &
echo "Step 3 (conditional)" |
echo "Fallback (if any step failed)";
echo "Final step (always runs)"

echo "Step 1"&echo "Step 2 (conditional)"&echo "Step 3 (conditional)"|echo "Fallback (if any step failed)";echo "Final step (always runs)"
